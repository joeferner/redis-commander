<div id="itemActionsBar">
  <button class="btn btn-danger" onclick="deleteKey('<%= key %>')">Delete Key</button>
</div>
<div id="itemData">
  <form id="editStringForm" action="/apiv1/key/<%= key %>" method="POST" class="form-vertical">
    <label>Value</label>
    <input type="hidden" id="isJson" name="isJson">
    <textarea id="stringValue" name="stringValue" style="width: 600px; height: 300px;"></textarea>
    <span id="stringValueClippy"></span>
    <br>
    <button id="saveKeyButton" onclick="$('#editStringForm').submit()">Save</button>
  </form>
</div>